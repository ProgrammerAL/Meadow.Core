	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\..\icon.png" Link="icon.png" Pack="true" PackagePath="" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\..\..\..\Meadow.Foundation\Source\Meadow.Foundation.Peripherals\ICs.IOExpanders.Ft232h\Driver\ICs.IOExpanders.Ft232h.csproj" />
